@model IEnumerable<ExamScoringApp.Models.StudentsPointsVM>



<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Students Points</h2>
        @*<ol class="breadcrumb">
            <li class="active">
                <strong>Students Points</strong>
            </li>
        </ol>*@
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>List of Students Points</h5>
                   
                </div>
                <div class="ibox-content">

                    <table id="table" class="table table-striped">
                        <tr>
                            <th>
                                @Html.DisplayNameFor(model => model.StudentNo)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.StudentName)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.CorrectTokens)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.AnsweredTokens)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.Points)
                            </th>
                        </tr>

                        @foreach (var item in Model)
                        {
                            <tr>
                                 <td>
                                    @Html.DisplayFor(modelItem => item.StudentNo)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.StudentName)
                                </td>
                                <td style="width:30%">
                                    @Html.DisplayFor(modelItem => item.CorrectTokens)
                                </td>
                                <td style="width:30%">
                                    @Html.DisplayFor(modelItem => item.AnsweredTokens)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Points)
                                </td>
                              
                            </tr>
                        }

                    </table>

                </div>
            </div>
        </div>
    </div>
</div>


